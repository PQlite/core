{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Main",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "env": {},
      "args": [],
      "showLog": true,
      "trace": "verbose"
    },
    {
      "name": "Launch Current File",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${file}",
      "showLog": true
    },
    {
      "name": "Launch with Custom Entry",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/main.go",
      "env": {
        "GO_ENV": "development"
      },
      "args": ["--port=8080"],
      "showLog": true
    }
  ]
}